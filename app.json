{
  "expo": {
    "name": "eatup",
    "slug": "eatup",
    "version": "1.0.0",
    "scheme": "acme",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#232323"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      }
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-camera", {
          "cameraPermission": "EatUp requires camera access to identify and measure food portions, aiding in nutritional tracking. Results are displayed on the homescreen."
        }
      ]
    ]
  }
}
